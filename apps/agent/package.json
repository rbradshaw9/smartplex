{
  "name": "@smartplex/agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "uvicorn app.main:app --reload --host 0.0.0.0 --port 9000",
    "start": "uvicorn app.main:app --host 0.0.0.0 --port 9000",
    "build": "echo 'SmartPlex Agent build complete'",
    "docker:build": "docker build -t smartplex-agent .",
    "docker:run": "docker run -p 9000:9000 --env-file .env smartplex-agent",
    "test": "pytest",
    "lint": "flake8 app/",
    "lint:fix": "black app/ && isort app/",
    "clean": "find . -name '*.pyc' -delete && find . -name '__pycache__' -delete"
  },
  "dependencies": {},
  "devDependencies": {}
}