{
  "version": 2,
  "installCommand": "npm install -g pnpm@8.15.0 && pnpm install",
  "buildCommand": "pnpm --filter @smartplex/web build",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}
